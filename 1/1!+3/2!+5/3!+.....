#include<stdio.h>
int main()
{

int i,n,j;
float up,down,term,sum=0;
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        up=0;
        down=1;
    }
    for(j=1;j<=i;j++)
        {
            up=up+j;
    down=down*j;


    }
    term=(float) up / down;
    sum=sum+term;
    printf("%f",sum);
    return;
}
